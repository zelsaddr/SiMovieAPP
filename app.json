{
  "expo": {
    "name": "SiMovieApp",
    "slug": "SiMovieApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/simovie_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/simovie_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.simovie.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.simovie.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/simovie_icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "packagerOpts": {
      "sourceExts": ["cjs", "js", "tsx"]
    }
  }
}
